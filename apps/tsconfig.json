/* To learn more about this file see: https://angular.io/config/tsconfig. */
{
  "compileOnSave": false,
  "compilerOptions": {
    "paths": {
      "@ci-apps/Arquivos": [
        "./dist/ci-apps/arquivos"
      ],
      "@ci-apps/Cadastros": [
        "./dist/ci-apps/cadastros"
      ],
      "@ci-apps/Codex": [
        "./dist/ci-apps/codex"
      ],
      "@ci-apps/DevTools": [
        "./dist/ci-apps/dev-tools"
      ],
      "@ci-apps/Dynamic": [
        "./dist/ci-apps/dynamic"
      ],
      "@ci-apps/Financeiro": [
        "./dist/ci-apps/financeiro"
      ],
      "@ci-apps/Formularios": [
        "./dist/ci-apps/formularios"
      ],
      "@ci-apps/Gerencial": [
        "./dist/ci-apps/gerencial"
      ],
      "@ci-apps/Icons": [
        "./dist/ci-apps/icons"
      ],
      "@ci-apps/Imersao": [
        "./dist/ci-apps/imersao"
      ],
      "@ci-apps/Infra": [
        "./dist/ci-apps/infra"
      ],
      "@ci-apps/Instalacao": [
        "./dist/ci-apps/instalacao"
      ],
      "@ci-apps/LowCode": [
        "./dist/ci-apps/low-code"
      ],
      "@ci-apps/Mensagens": [
        "./dist/ci-apps/mensagens"
      ],
      "@ci-apps/Organizacao": [
        "./dist/ci-apps/organizacao"
      ],
      "@ci-apps/Produtos": [
        "./dist/ci-apps/produtos"
      ],
      "@ci-apps/Profile": [
        "./dist/ci-apps/profile"
      ],
      "@ci-apps/Projetos": [
        "./dist/ci-apps/projetos"
      ],
      "@ci-apps/SEO": [
        "./dist/ci-apps/seo"
      ],
      "@ci-apps/Threejs": [
        "./dist/ci-apps/threejs"
      ],
      "@ci-apps/Treinamento": [
        "./dist/ci-apps/treinamento"
      ],
      "@ci-apps/Vendas": [
        "./dist/ci-apps/vendas"
      ],
      "@ci/components": [
        "./dist/ci/components"
      ],
      "@ci/core": [
        "./dist/ci/core"
      ],
      "@ci/data-grid": [
        "./dist/ci/data-grid"
      ],
      "@ci/portal-api": [
        "./dist/ci/portal-api"
      ]
    },
    "outDir": "./dist/out-tsc",
    "strict": true,
    "noImplicitOverride": true,
    "noPropertyAccessFromIndexSignature": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "sourceMap": true,
    "declaration": false,
    "experimentalDecorators": true,
    "moduleResolution": "bundler",
    "importHelpers": true,
    "target": "ES2022",
    "module": "ES2022",
    "useDefineForClassFields": false,
    "lib": [
      "ES2022",
      "dom"
    ],
  },
  "angularCompilerOptions": {
    "enableI18nLegacyMessageIdFormat": false,
    "strictInjectionParameters": true,
    "strictInputAccessModifiers": true,
    "strictTemplates": true
  },
  "exclude": [
    "./dist/**/**",
    ".../../../dist/**/**",
    "./projects/**/**"
  ]
}